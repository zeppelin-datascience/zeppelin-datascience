    {
      "text": "%sql\nWITH\ntarget (\nSELECT\n    ${column} AS x,\n    ${group} AS grp\nFROM survey\nWHERE ${column} IS NOT NULL\n),\nstats (\nSELECT\n    MIN(x) AS x_min,\n    ${numBins} AS num_bins,\n    (MAX(x) - MIN(x)) / ${numBins} AS bin_width\nFROM target\n),\nbin_tmp (\nSELECT\n    x, grp,\n    FLOOR((x - x_min) / bin_width) AS bin\nFROM\n    target\n    CROSS JOIN stats\n),\nbin (\nSELECT\n    x, grp, \n    CASE WHEN bin \u003d num_bins THEN num_bins - 1 ELSE bin END AS bin  -- 마지막 bin의 범위를 90 ~ 99이 아닌 90 ~ 100이 되도록\nFROM\n    bin_tmp\n    CROSS JOIN stats\n),\nbin_stats (\nSELECT\n    bin,\n    AVG(x) AS bin_avg,\n    MIN(x) AS bin_min,\n    MAX(x) AS bin_max,\n    MIN(bin_width) * bin AS bin_start,\n    MIN(bin_width) * (bin + 1) AS bin_end\nFROM\n    bin\n    CROSS JOIN stats\nGROUP BY bin\n),\nbin_group (\nSELECT bin, grp, COUNT(*) AS cnt\nFROM bin\nGROUP BY bin, grp\n)\nSELECT bin, grp, cnt, bin_avg, bin_min, bin_max, bin_start, bin_end\nFROM\n    bin_group\n    JOIN bin_stats USING (bin)\nORDER BY bin, grp",
